{
    "name": "peerzs",
    "type": "module",
    "version": "0.0.0",
    "private": true,
    "packageManager": "pnpm@9.1.2",
    "description": "Instantly share files with peers using Peerzs",
    "author": "XONE Labs",
    "license": "GPL-3.0",
    "respository": {
        "type": "git",
        "url": "https://github.com/xone-labs/peerzs.git"
    },
    "bugs": {
        "url": "https://github.com/xone-labs/peerzs/issues"
    },
    "keywords": [
