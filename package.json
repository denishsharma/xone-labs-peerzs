        "@unocss/nuxt": "^0.60.4",
        "@vueuse/core": "^10.10.0",
        "@vueuse/nuxt": "^10.10.0",
        "eslint": "^8.57.0",
        "eslint-plugin-format": "^0.1.1",
        "lint-staged": "^15.2.5",
        "nuxt-icon": "^0.6.10",
        "nuxt-svgo": "^4.0.1",
        "sass": "^1.77.4",
        "simple-git-hooks": "^2.11.1",
        "std-env": "^3.7.0",
